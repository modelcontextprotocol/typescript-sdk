{
    "name": "mcp-sdk-conformance-tests",
    "private": true,
    "type": "module",
    "scripts": {
        "test:server": "./scripts/run-server-conformance.sh --expected-failures ./conformance-baseline.yml",
        "test:server:all": "./scripts/run-server-conformance.sh --suite all --expected-failures ./conformance-baseline.yml",
        "test:server:run": "npx tsx ./src/everythingServer.ts",
        "test:client": "npx @modelcontextprotocol/conformance client --command 'npx tsx ./src/everythingClient.ts' --expected-failures ./conformance-baseline.yml",
        "test:client:all": "npx @modelcontextprotocol/conformance client --command 'npx tsx ./src/everythingClient.ts' --suite all --expected-failures ./conformance-baseline.yml"
    },
    "devDependencies": {
        "@modelcontextprotocol/conformance": "0.1.11",
        "tsx": "^4.19.4"
    }
}
